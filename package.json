  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "ascii-table": "0.0.9",
    "axios": "1.7.9",
    "commander": "13.1.0",
    "dotenv": "16.4.7",
    "lodash": "4.17.21",
    "ora": "8.2.0"
  }
}
